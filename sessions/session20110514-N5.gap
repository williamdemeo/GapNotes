# [ 216, 108, 72, 72, 72, 54, 36, 36, 27, 24, 24, 24, 12, 12, 9, 8, 6, 4, 3, 1 ]
H:=Representative(ccsg[8]);
intHG:=IntermediateSubgroups(G,H);
# rec( subgroups := [ Group([ f1, f4, f5*f6 ]), Group([ f1, f2, f3*f4, f4 ]), Group([ f1, f4, f5, f6 ]) ], 
#     inclusions := [ [ 0, 1 ], [ 0, 2 ], [ 1, 3 ], [ 2, 4 ], [ 3, 4 ] ] )
